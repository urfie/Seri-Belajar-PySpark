            "|Windy|   Umum|  300|\n",
            "| Budi|   Umum|  300|\n",
            "| Dina|   Umum|  250|\n",
            "| Bayu|   Umum|  150|\n",
            "| Dedi|   Umum|  150|\n",
            "+-----+-------+-----+\n",
            "\n"
          ]
        }
      ]
